{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "Nuestro primer proyecto de Nodejs",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server", 
    "start": "concurrently \"npm run watch:scss\" \"npm run dev \"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.css' -- npm run",
    "clean": "rimraf dist",
    "imagesmin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin precios.html -d dist --htmlmin -o dist/precios.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html",
    "build":"npm run clean && npm run scss && npm run imagesmin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidrestrepo07/Guia-hoteles.git"
  },
  "author": "David Restrepo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidrestrepo07/Guia-hoteles/issues"
  },
  "homepage": "https://github.com/davidrestrepo07/Guia-hoteles#readme",
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "concurrently": "^5.2.0",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1",
    "sass": "^1.26.8",
    "usemin": "^0.6.0"
  }
}
